{
  "name": "element-test",
  "version": "1.0.0",
  "scripts": {
    "dev": "dojo build --mode dev --watch --serve",
    "build": "dojo build widget --mode dev",
    "build:dev": "dojo build widget --mode dev",
    "test": "dojo test",
    "test:unit": "dojo build --mode unit && dojo test --unit --config local",
    "test:functional": "dojo build --mode functional && dojo test --functional --config local",
    "test:all": "dojo build --mode unit && dojo build --mode functional && dojo test --all --config local",
    "latest-contrib": "npm i ~/Desktop/Projects/webpack-contrib/dist/dojo-webpack-contrib-7.0.0.tgz && rm -rf node_modules/@dojo/cli-build-widget/node_modules/@dojo"
  },
  "dependencies": {
    "@dojo/cli-build-widget": "^7.0.0-alpha.6",
    "@dojo/framework": "^6.0.4",
    "@dojo/themes": "^6.0.0",
    "@dojo/webpack-contrib": "^6.0.5",
    "@dojo/widgets": "^6.0.0",
    "http-server": "^0.12.1",
    "tslib": "~1.9.1"
  },
  "devDependencies": {
    "@dojo/cli": "^6.0.0",
    "@dojo/cli-build-app": "^6.0.9",
    "@dojo/cli-test-intern": "^6.0.0",
    "@types/node": "~9.6.5",
    "typescript": "~3.4.5"
  }
}
